{
  "rewrites": [
    {
      "source": "/((?!.*\\.(json|js|css|jpg|jpeg|png|svg|ico|woff|woff2|ttf|eot|webmanifest)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(jpg|jpeg|png|svg|ico|json|css|js|woff|woff2|ttf|eot|webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}